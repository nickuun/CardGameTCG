; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="New Game Project"
run/main_scene="res://Scenes&Scripts/Battleground/battleground_scene.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

CardDatabase="*res://Scenes&Scripts/Cards/CardDatabase/Singleton/CardDatabase.gd"
Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
MultiplayerActionInterpreter="*res://Scenes&Scripts/Multiplayer/MultiplayerActionInterpreter.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=480
window/size/window_height_override=270
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[global_group]

cards=""
zones=""
AbilityManager=""
CardPreview=""

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
2d/snap/snap_2d_transforms_to_pixel=true
